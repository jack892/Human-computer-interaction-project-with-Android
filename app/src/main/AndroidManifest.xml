<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ium.swag.yolo.saxs">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Speech_to_gesture"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".OutputS2G"
            android:label="@string/app_name"
            android:parentActivityName=".Speech_to_gesture"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".IstruzioniWeb"
            android:parentActivityName=".Istruzioni"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Istruzioni"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Gesture_to_speech"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="landscape"/>
    </application>

</manifest>